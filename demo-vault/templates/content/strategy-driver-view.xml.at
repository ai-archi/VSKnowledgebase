<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-driver-view-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统业务驱动力图</archimate:name>
  <archimate:documentation>业务驱动力图展示影响ERP系统的驱动因素</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-driver-1" type="Driver">
      <archimate:name>市场竞争压力</archimate:name>
      <archimate:documentation>市场竞争压力推动ERP系统建设</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-driver-2" type="Driver">
      <archimate:name>数字化转型需求</archimate:name>
      <archimate:documentation>企业数字化转型需求驱动ERP系统实施</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-driver-3" type="Driver">
      <archimate:name>业务增长需求</archimate:name>
      <archimate:documentation>业务快速增长需要ERP系统支撑</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-driver-diagram">
        <archimate:name>业务驱动力图 (Driver View)</archimate:name>
        <archimate:documentation>ERP系统业务驱动力图 - 展示影响组织的驱动因素</archimate:documentation>
        <archimate:node id="id-driver-node-1" type="Element" elementRef="id-driver-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-driver-node-2" type="Element" elementRef="id-driver-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-driver-node-3" type="Element" elementRef="id-driver-3" x="500" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-driver-rel-1" type="Influence" source="id-driver-1" target="id-driver-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-driver-rel-2" type="Influence" source="id-driver-2" target="id-driver-3">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

