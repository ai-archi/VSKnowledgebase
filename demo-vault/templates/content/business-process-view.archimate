<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-business-process-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统业务流程图</archimate:name>
  <archimate:documentation>业务流程图展示ERP系统的核心业务流程</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-process-1" type="BusinessProcess">
      <archimate:name>订单处理流程</archimate:name>
      <archimate:documentation>从订单接收到订单完成的业务流程</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-process-2" type="BusinessProcess">
      <archimate:name>采购流程</archimate:name>
      <archimate:documentation>从采购申请到收货入库的业务流程</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-process-3" type="BusinessProcess">
      <archimate:name>财务结算流程</archimate:name>
      <archimate:documentation>财务结算和账务处理流程</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-process-diagram">
        <archimate:name>业务流程图 (Business Process View)</archimate:name>
        <archimate:documentation>ERP系统业务流程图 - 展示核心业务流程</archimate:documentation>
        <archimate:node id="id-process-node-1" type="Element" elementRef="id-process-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-process-node-2" type="Element" elementRef="id-process-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-process-node-3" type="Element" elementRef="id-process-3" x="500" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-process-rel-1" type="Flow" source="id-process-1" target="id-process-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-process-rel-2" type="Flow" source="id-process-2" target="id-process-3">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

