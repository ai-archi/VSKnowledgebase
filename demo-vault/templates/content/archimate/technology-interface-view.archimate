<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-technology-interface-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统技术接口图</archimate:name>
  <archimate:documentation>技术接口图展示ERP系统的技术接口及其连接关系</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-interface-1" type="TechnologyInterface">
      <archimate:name>HTTP接口</archimate:name>
      <archimate:documentation>Web服务器提供的HTTP接口</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-interface-2" type="TechnologyInterface">
      <archimate:name>数据库接口</archimate:name>
      <archimate:documentation>数据库服务器提供的接口</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-component-1" type="TechnologyComponent">
      <archimate:name>Web服务器组件</archimate:name>
      <archimate:documentation>Web服务器技术组件</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-interface-diagram">
        <archimate:name>技术接口图 (Technology Interface View)</archimate:name>
        <archimate:documentation>ERP系统技术接口图 - 展示技术接口及其连接关系</archimate:documentation>
        <archimate:node id="id-interface-node-1" type="Element" elementRef="id-interface-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="221" g="235" b="247" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-interface-node-2" type="Element" elementRef="id-interface-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="221" g="235" b="247" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-component-node-1" type="Element" elementRef="id-component-1" x="200" y="250" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="221" g="235" b="247" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-interface-rel-1" type="Realization" source="id-component-1" target="id-interface-1">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-interface-rel-2" type="Serving" source="id-interface-1" target="id-interface-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

