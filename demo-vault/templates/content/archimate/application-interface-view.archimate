<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-application-interface-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统应用接口图</archimate:name>
  <archimate:documentation>应用接口图展示ERP系统的应用接口及其连接关系</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-interface-1" type="ApplicationInterface">
      <archimate:name>订单API接口</archimate:name>
      <archimate:documentation>订单管理模块提供的API接口</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-interface-2" type="ApplicationInterface">
      <archimate:name>库存API接口</archimate:name>
      <archimate:documentation>库存管理模块提供的API接口</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-component-1" type="ApplicationComponent">
      <archimate:name>订单管理应用</archimate:name>
      <archimate:documentation>订单管理应用组件</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-interface-diagram">
        <archimate:name>应用接口图 (Application Interface View)</archimate:name>
        <archimate:documentation>ERP系统应用接口图 - 展示应用接口及其连接关系</archimate:documentation>
        <archimate:node id="id-interface-node-1" type="Element" elementRef="id-interface-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="173" g="216" b="230" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-interface-node-2" type="Element" elementRef="id-interface-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="173" g="216" b="230" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-component-node-1" type="Element" elementRef="id-component-1" x="200" y="250" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="173" g="216" b="230" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-interface-rel-1" type="Realization" source="id-component-1" target="id-interface-1">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-interface-rel-2" type="Serving" source="id-interface-1" target="id-interface-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

