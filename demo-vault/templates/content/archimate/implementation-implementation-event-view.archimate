<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-implementation-event-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统实施事件图</archimate:name>
  <archimate:documentation>实施事件图展示ERP系统实施过程中的关键事件</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-event-1" type="BusinessEvent">
      <archimate:name>项目启动事件</archimate:name>
      <archimate:documentation>ERP系统项目启动的关键事件</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-event-2" type="BusinessEvent">
      <archimate:name>系统上线事件</archimate:name>
      <archimate:documentation>ERP系统正式上线的关键事件</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-event-3" type="BusinessEvent">
      <archimate:name>验收完成事件</archimate:name>
      <archimate:documentation>ERP系统验收完成的关键事件</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-implementation-event-diagram">
        <archimate:name>实施事件图 (Implementation Event View)</archimate:name>
        <archimate:documentation>ERP系统实施事件图 - 展示实施过程中的关键事件</archimate:documentation>
        <archimate:node id="id-event-node-1" type="Element" elementRef="id-event-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="228" b="225" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-event-node-2" type="Element" elementRef="id-event-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="228" b="225" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-event-node-3" type="Element" elementRef="id-event-3" x="500" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="228" b="225" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-implementation-event-rel-1" type="Triggering" source="id-event-1" target="id-event-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-implementation-event-rel-2" type="Triggering" source="id-event-2" target="id-event-3">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

