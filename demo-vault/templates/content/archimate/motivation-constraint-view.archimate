<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-constraint-view-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统约束视图</archimate:name>
  <archimate:documentation>约束视图展示ERP系统设计和实施面临的约束条件</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-constraint-1" type="Constraint">
      <archimate:name>预算约束</archimate:name>
      <archimate:documentation>ERP系统实施预算限制</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-constraint-2" type="Constraint">
      <archimate:name>时间约束</archimate:name>
      <archimate:documentation>ERP系统实施时间限制</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-constraint-3" type="Constraint">
      <archimate:name>技术约束</archimate:name>
      <archimate:documentation>现有技术架构对ERP系统的约束</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-constraint-diagram">
        <archimate:name>约束视图 (Constraint View)</archimate:name>
        <archimate:documentation>ERP系统约束视图 - 展示设计和实施面临的约束条件</archimate:documentation>
        <archimate:node id="id-constraint-node-1" type="Element" elementRef="id-constraint-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-constraint-node-2" type="Element" elementRef="id-constraint-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-constraint-node-3" type="Element" elementRef="id-constraint-3" x="500" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-constraint-rel-1" type="Association" source="id-constraint-1" target="id-constraint-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-constraint-rel-2" type="Association" source="id-constraint-2" target="id-constraint-3">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

