<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-strategy-view-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统战略视图</archimate:name>
  <archimate:documentation>战略视图展示ERP系统的战略方向和行动方案</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-strategy-1" type="CourseOfAction">
      <archimate:name>数字化转型战略</archimate:name>
      <archimate:documentation>通过ERP系统实现企业数字化转型</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-strategy-2" type="CourseOfAction">
      <archimate:name>业务流程优化</archimate:name>
      <archimate:documentation>优化企业核心业务流程</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-resource-1" type="Resource">
      <archimate:name>ERP系统资源</archimate:name>
      <archimate:documentation>ERP系统所需的关键资源</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-strategy-diagram">
        <archimate:name>战略视图 (Strategy View)</archimate:name>
        <archimate:documentation>ERP系统战略视图 - 展示战略方向和行动方案</archimate:documentation>
        <archimate:node id="id-strategy-node-1" type="Element" elementRef="id-strategy-1" x="200" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="245" g="222" b="170" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-strategy-node-2" type="Element" elementRef="id-strategy-2" x="400" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="245" g="222" b="170" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-resource-node-1" type="Element" elementRef="id-resource-1" x="300" y="250" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="245" g="222" b="170" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-strategy-rel-1" type="Realization" source="id-resource-1" target="id-strategy-1">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-strategy-rel-2" type="Influence" source="id-strategy-1" target="id-strategy-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

