<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-goal-view-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统业务目标图</archimate:name>
  <archimate:documentation>业务目标图展示ERP系统的业务目标</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-goal-1" type="Goal">
      <archimate:name>提升运营效率</archimate:name>
      <archimate:documentation>通过ERP系统提升企业运营效率</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-goal-2" type="Goal">
      <archimate:name>降低运营成本</archimate:name>
      <archimate:documentation>通过ERP系统降低企业运营成本</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-goal-3" type="Goal">
      <archimate:name>提高决策质量</archimate:name>
      <archimate:documentation>通过ERP系统提供的数据支持提高决策质量</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-goal-diagram">
        <archimate:name>业务目标图 (Goal View)</archimate:name>
        <archimate:documentation>ERP系统业务目标图 - 展示组织的目标</archimate:documentation>
        <archimate:node id="id-goal-node-1" type="Element" elementRef="id-goal-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-goal-node-2" type="Element" elementRef="id-goal-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-goal-node-3" type="Element" elementRef="id-goal-3" x="500" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-goal-rel-1" type="Association" source="id-goal-1" target="id-goal-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-goal-rel-2" type="Association" source="id-goal-2" target="id-goal-3">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

