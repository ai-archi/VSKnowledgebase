<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-migration-roadmap-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统迁移路线图</archimate:name>
  <archimate:documentation>迁移路线图展示ERP系统的迁移路径和里程碑</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-plateau-1" type="Plateau">
      <archimate:name>当前状态</archimate:name>
      <archimate:documentation>ERP系统迁移前的当前状态</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-plateau-2" type="Plateau">
      <archimate:name>过渡状态</archimate:name>
      <archimate:documentation>ERP系统迁移过程中的过渡状态</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-plateau-3" type="Plateau">
      <archimate:name>目标状态</archimate:name>
      <archimate:documentation>ERP系统迁移后的目标状态</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-migration-roadmap-diagram">
        <archimate:name>迁移路线图 (Migration Roadmap View)</archimate:name>
        <archimate:documentation>ERP系统迁移路线图 - 展示迁移路径和里程碑</archimate:documentation>
        <archimate:node id="id-plateau-node-1" type="Element" elementRef="id-plateau-1" x="100" y="150" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="228" b="225" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-plateau-node-2" type="Element" elementRef="id-plateau-2" x="300" y="150" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="228" b="225" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-plateau-node-3" type="Element" elementRef="id-plateau-3" x="500" y="150" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="228" b="225" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-migration-rel-1" type="Triggering" source="id-plateau-1" target="id-plateau-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-migration-rel-2" type="Triggering" source="id-plateau-2" target="id-plateau-3">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

