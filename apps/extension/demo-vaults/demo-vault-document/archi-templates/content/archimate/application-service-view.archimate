<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-application-service-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统应用服务图</archimate:name>
  <archimate:documentation>应用服务图展示ERP系统提供的应用服务</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-service-1" type="ApplicationService">
      <archimate:name>订单服务</archimate:name>
      <archimate:documentation>提供订单管理的应用服务</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-service-2" type="ApplicationService">
      <archimate:name>库存服务</archimate:name>
      <archimate:documentation>提供库存管理的应用服务</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-service-3" type="ApplicationService">
      <archimate:name>财务服务</archimate:name>
      <archimate:documentation>提供财务管理的应用服务</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-service-diagram">
        <archimate:name>应用服务图 (Application Service View)</archimate:name>
        <archimate:documentation>ERP系统应用服务图 - 展示提供的应用服务</archimate:documentation>
        <archimate:node id="id-service-node-1" type="Element" elementRef="id-service-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="173" g="216" b="230" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-service-node-2" type="Element" elementRef="id-service-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="173" g="216" b="230" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-service-node-3" type="Element" elementRef="id-service-3" x="500" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="173" g="216" b="230" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-service-rel-1" type="Serving" source="id-service-1" target="id-service-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-service-rel-2" type="Serving" source="id-service-2" target="id-service-3">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

