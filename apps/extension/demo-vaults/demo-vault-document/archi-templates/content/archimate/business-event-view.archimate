<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-business-event-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统业务事件图</archimate:name>
  <archimate:documentation>业务事件图展示ERP系统中触发业务流程的事件</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-event-1" type="BusinessEvent">
      <archimate:name>订单创建事件</archimate:name>
      <archimate:documentation>客户下单时触发的业务事件</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-event-2" type="BusinessEvent">
      <archimate:name>库存不足事件</archimate:name>
      <archimate:documentation>库存低于阈值时触发的业务事件</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-event-3" type="BusinessEvent">
      <archimate:name>付款完成事件</archimate:name>
      <archimate:documentation>客户付款完成时触发的业务事件</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-event-diagram">
        <archimate:name>业务事件图 (Business Event View)</archimate:name>
        <archimate:documentation>ERP系统业务事件图 - 展示触发业务流程的事件</archimate:documentation>
        <archimate:node id="id-event-node-1" type="Element" elementRef="id-event-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-event-node-2" type="Element" elementRef="id-event-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-event-node-3" type="Element" elementRef="id-event-3" x="500" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-event-rel-1" type="Triggering" source="id-event-1" target="id-event-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-event-rel-2" type="Triggering" source="id-event-2" target="id-event-3">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

