<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-cross-layered-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统层次视图</archimate:name>
  <archimate:documentation>层次视图展示ERP系统的分层架构</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-business-service-1" type="BusinessService">
      <archimate:name>订单服务</archimate:name>
      <archimate:documentation>业务层的订单服务</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-application-service-1" type="ApplicationService">
      <archimate:name>订单应用服务</archimate:name>
      <archimate:documentation>应用层的订单应用服务</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-technology-service-1" type="TechnologyService">
      <archimate:name>数据服务</archimate:name>
      <archimate:documentation>技术层的数据服务</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-layered-diagram">
        <archimate:name>层次视图 (Layered / Hierarchy View)</archimate:name>
        <archimate:documentation>ERP系统层次视图 - 展示分层架构</archimate:documentation>
        <archimate:node id="id-business-service-node-1" type="Element" elementRef="id-business-service-1" x="250" y="50" w="200" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-application-service-node-1" type="Element" elementRef="id-application-service-1" x="250" y="150" w="200" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="173" g="216" b="230" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-technology-service-node-1" type="Element" elementRef="id-technology-service-1" x="250" y="250" w="200" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="221" g="235" b="247" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-layered-rel-1" type="Realization" source="id-application-service-1" target="id-business-service-1">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-layered-rel-2" type="Realization" source="id-technology-service-1" target="id-application-service-1">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

