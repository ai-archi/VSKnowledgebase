<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-value-view-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统价值视图</archimate:name>
  <archimate:documentation>价值视图展示ERP系统为组织创造的价值</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-value-1" type="Value">
      <archimate:name>运营效率提升</archimate:name>
      <archimate:documentation>ERP系统带来的运营效率提升价值</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-value-2" type="Value">
      <archimate:name>成本降低</archimate:name>
      <archimate:documentation>ERP系统带来的成本降低价值</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-value-3" type="Value">
      <archimate:name>决策支持</archimate:name>
      <archimate:documentation>ERP系统提供的决策支持价值</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-value-diagram">
        <archimate:name>价值视图 (Value View)</archimate:name>
        <archimate:documentation>ERP系统价值视图 - 展示为组织创造的价值</archimate:documentation>
        <archimate:node id="id-value-node-1" type="Element" elementRef="id-value-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-value-node-2" type="Element" elementRef="id-value-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-value-node-3" type="Element" elementRef="id-value-3" x="500" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-value-rel-1" type="Association" source="id-value-1" target="id-value-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-value-rel-2" type="Association" source="id-value-2" target="id-value-3">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

