<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-application-data-object-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统数据对象图</archimate:name>
  <archimate:documentation>数据对象图展示ERP系统中的数据对象及其关系</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-data-1" type="DataObject">
      <archimate:name>订单数据对象</archimate:name>
      <archimate:documentation>订单相关的数据对象</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-data-2" type="DataObject">
      <archimate:name>产品数据对象</archimate:name>
      <archimate:documentation>产品相关的数据对象</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-data-3" type="DataObject">
      <archimate:name>客户数据对象</archimate:name>
      <archimate:documentation>客户相关的数据对象</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-data-object-diagram">
        <archimate:name>数据对象图 (Data Object View)</archimate:name>
        <archimate:documentation>ERP系统数据对象图 - 展示数据对象及其关系</archimate:documentation>
        <archimate:node id="id-data-node-1" type="Element" elementRef="id-data-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="173" g="216" b="230" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-data-node-2" type="Element" elementRef="id-data-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="173" g="216" b="230" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-data-node-3" type="Element" elementRef="id-data-3" x="500" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="173" g="216" b="230" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-data-rel-1" type="Association" source="id-data-1" target="id-data-2">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-data-rel-2" type="Association" source="id-data-1" target="id-data-3">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

