<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-business-interaction-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统业务交互图</archimate:name>
  <archimate:documentation>业务交互图展示ERP系统中业务角色之间的交互</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-role-1" type="BusinessRole">
      <archimate:name>销售员</archimate:name>
      <archimate:documentation>负责销售业务的角色</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-role-2" type="BusinessRole">
      <archimate:name>仓库管理员</archimate:name>
      <archimate:documentation>负责库存管理的角色</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-interaction-1" type="BusinessInteraction">
      <archimate:name>订单处理交互</archimate:name>
      <archimate:documentation>销售员与仓库管理员之间的订单处理交互</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-interaction-diagram">
        <archimate:name>业务交互图 (Business Interaction View)</archimate:name>
        <archimate:documentation>ERP系统业务交互图 - 展示业务角色之间的交互</archimate:documentation>
        <archimate:node id="id-role-node-1" type="Element" elementRef="id-role-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-role-node-2" type="Element" elementRef="id-role-2" x="400" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-interaction-node-1" type="Element" elementRef="id-interaction-1" x="250" y="200" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-interaction-rel-1" type="Assignment" source="id-role-1" target="id-interaction-1">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-interaction-rel-2" type="Assignment" source="id-role-2" target="id-interaction-1">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

