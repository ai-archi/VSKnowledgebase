<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:archimate="http://www.opengroup.org/xsd/archimate/3.0/" id="id-motivation-goal-model" xsi:schemaLocation="http://www.opengroup.org/xsd/archimate/3.0/ http://www.opengroup.org/xsd/archimate/3.1/archimate3_Diagram.xsd">
  <archimate:name>ERP系统目标视图</archimate:name>
  <archimate:documentation>目标视图展示ERP系统的业务目标和期望结果</archimate:documentation>
  <archimate:elements>
    <archimate:baseElement id="id-goal-1" type="Goal">
      <archimate:name>实现业务流程自动化</archimate:name>
      <archimate:documentation>通过ERP系统实现核心业务流程自动化</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-goal-2" type="Goal">
      <archimate:name>提升数据准确性</archimate:name>
      <archimate:documentation>通过ERP系统提升业务数据准确性</archimate:documentation>
    </archimate:baseElement>
    <archimate:baseElement id="id-outcome-1" type="Outcome">
      <archimate:name>运营效率提升30%</archimate:name>
      <archimate:documentation>ERP系统实施后的预期成果</archimate:documentation>
    </archimate:baseElement>
  </archimate:elements>
  <archimate:views>
    <archimate:diagrams>
      <archimate:view id="id-goal-diagram">
        <archimate:name>目标视图 (Goal View)</archimate:name>
        <archimate:documentation>ERP系统目标视图 - 展示业务目标和期望结果</archimate:documentation>
        <archimate:node id="id-goal-node-1" type="Element" elementRef="id-goal-1" x="100" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-goal-node-2" type="Element" elementRef="id-goal-2" x="300" y="100" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
        <archimate:node id="id-outcome-node-1" type="Element" elementRef="id-outcome-1" x="200" y="250" w="150" h="60">
          <archimate:style textAlign="center-middle">
            <archimate:font />
            <archimate:fillColor r="255" g="255" b="181" a="100" />
            <archimate:lineColor r="0" g="0" b="0" a="40" />
          </archimate:style>
        </archimate:node>
      </archimate:view>
    </archimate:diagrams>
  </archimate:views>
  <archimate:relationships>
    <archimate:relationship id="id-goal-rel-1" type="Realization" source="id-goal-1" target="id-outcome-1">
      <archimate:name></archimate:name>
    </archimate:relationship>
    <archimate:relationship id="id-goal-rel-2" type="Realization" source="id-goal-2" target="id-outcome-1">
      <archimate:name></archimate:name>
    </archimate:relationship>
  </archimate:relationships>
</archimate:model>

