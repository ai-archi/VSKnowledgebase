<?xml version="1.0" encoding="UTF-8"?>
<archimate:model xmlns:archimate="http://www.archimatetool.com/archimate" 
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                 xsi:schemaLocation="http://www.archimatetool.com/archimate http://www.archimatetool.com/archimate" 
                 id="example-model" name="Example ArchiMate Model" version="3.1">
  <properties>
    <property key="description">Example ArchiMate model for testing</property>
  </properties>
  <elements>
    <applicationComponent id="app-component-1" name="User Service" type="ApplicationComponent"/>
    <applicationComponent id="app-component-2" name="Payment Service" type="ApplicationComponent"/>
  </elements>
  <relationships>
    <relationship id="rel-1" source="app-component-1" target="app-component-2" type="ServingRelationship"/>
  </relationships>
  <views>
    <view id="view-1" name="Main View" type="Canvas">
      <nodes>
        <node elementRef="app-component-1" x="100" y="100"/>
        <node elementRef="app-component-2" x="300" y="100"/>
      </nodes>
      <connections>
        <connection relationshipRef="rel-1"/>
      </connections>
    </view>
  </views>
</archimate:model>

